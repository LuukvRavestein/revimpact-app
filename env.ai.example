# AI Feature Environment Variables
# Add these to your Vercel environment variables

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# AI Feature Settings
AI_FEATURE_ENABLED=true
AI_MAX_ANALYSIS_RECORDS=1000
AI_CONFIDENCE_THRESHOLD=0.7

# Privacy Controls
AI_DATA_RETENTION_DAYS=30
AI_ALLOW_EXTERNAL_ANALYSIS=false
AI_DATA_ANONYMIZATION_LEVEL=partial

# Dashboard Generation
AI_MAX_DASHBOARDS_PER_WORKSPACE=10
AI_DEFAULT_DASHBOARD_THEME=light
