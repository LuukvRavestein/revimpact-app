{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/((?!app\\.).*)",
      "destination": "/marketing",
      "has": [
        {
          "type": "host",
          "value": "www.revimpact.nl"
        }
      ]
    }
  ]
}
